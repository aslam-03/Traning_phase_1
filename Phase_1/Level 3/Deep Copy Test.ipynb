    "import copy\n",
    "\n",
    "lst1=[[1,2],[3,4]]\n",
    "\n",
    "shallow_lst=lst1.copy()\n",
    "\n",
    "deep_lst=copy.deepcopy(lst1)\n",
    "\n",
    "shallow_lst[0][0]=9\n",
    "\n",
    "deep_lst[0][0]=8\n",
    "\n",
    "print(\"Original list:\",lst1,\"Shallow copy list:\",shallow_lst,\"Deep copy List\",deep_lst)\n"
   ]
